{
	"name": "binarybuilds/nova-mail-manager",
	"description": "A Laravel Nova tool to easily resend system generated mails without going through application flow.",
	"keywords": [
		"laravel",
		"nova",
		"mail",
		"email",
		"monitoring"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Srinath Reddy Dudi",
			"email": "srinathreddydudi@gmail.com"
		}
	],
	"require": {
		"php": ">=8.1",
		"binarybuilds/laravel-mail-manager": "^1.0"
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/TwolfNL/laravel-mail-manager"
		}
	],
	"autoload": {
		"psr-4": {
			"BinaryBuilds\\NovaMailManager\\": "src/"
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
